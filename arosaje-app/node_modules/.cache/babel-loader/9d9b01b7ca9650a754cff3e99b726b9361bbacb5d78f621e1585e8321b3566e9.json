{"ast":null,"code":"import { Field, ErrorMessage } from \"vee-validate\";\nimport * as yup from \"yup\";\nimport axios from 'axios';\nexport default {\n  name: \"LoginComponent\",\n  components: {\n    Field,\n    ErrorMessage\n  },\n  data() {\n    const schema = yup.object().shape({\n      email: yup.string().required(\"Veiller entrer votre identifiant\"),\n      password: yup.string().required(\"Veiller entrer votre mot de passe\")\n    });\n    return {\n      loading: false,\n      message: \"\",\n      schema,\n      email: '',\n      password: ''\n    };\n  },\n  methods: {\n    async onSubmit() {\n      try {\n        // Envoyer la requête d'authentification à l'API\n        const response = await axios.post('http://localhost:3000/signup', {\n          nom: this.nom,\n          prenom: this.prenom,\n          email: this.email,\n          mdp: this.mdp\n        });\n        // Traiter la réponse de l'API\n\n        const data = await response.json();\n        console.log(data);\n        if (response.ok) {\n          // Enregistrer le jeton d'authentification dans le stockage local ou dans un cookie\n          localStorage.setItem('authToken', data.token);\n\n          // Rediriger l'utilisateur vers la page suivante\n          // this.$router.push('/profil');\n        } else {\n          // Afficher un message d'erreur à l'utilisateur\n          alert(data.message);\n        }\n      } catch (error) {\n        console.error(error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["Field","ErrorMessage","yup","axios","name","components","data","schema","object","shape","email","string","required","password","loading","message","methods","onSubmit","response","post","nom","prenom","mdp","json","console","log","ok","localStorage","setItem","token","alert","error"],"sources":["C:\\Users\\abduk\\Downloads\\epsi\\mspr_dev\\arosaje-app\\src\\views\\LoginComponent.vue"],"sourcesContent":["<template>\r\n  <div class=\"containnerform\">\r\n    <div class=\"bloc\">\r\n\r\n      <div class=\"form\">\r\n        <img src=\"https://e7.pngegg.com/pngimages/293/425/png-clipart-natural-environment-plant-%E4%B8%80%E5%80%8B%E6%96%B0%E4%B8%96%E7%95%8C-%E5%96%9A%E9%86%92%E5%85%A7%E5%9C%A8%E7%9A%84%E5%8A%9B%E9%87%8F-orchids-soil-natural-environment-leaf-logo.png\" alt=\"logoaro\"/>\r\n\r\n\r\n      <form  @submit.prevent=\"onSubmit\" :validation-schema=\"schema\" >\r\n        <div class=\"form-group\" >\r\n          <h3 class=\"titre\"> Connectez-vous !</h3><br>\r\n          <label for=\"username\">Email</label>\r\n          <Field  type=\"email\"  v-model=\"email\"  name=\"email\" class=\"form-control\" />\r\n          <ErrorMessage name=\"email\" class=\"error-feedback\" />\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"password\">Mot de passe</label><br>\r\n          <Field type=\"password\" id=\"password\" v-model=\"password\" name=\"password\"  class=\"form-control\" />\r\n          <ErrorMessage name=\"password\" class=\"error-feedback\" />\r\n        </div>\r\n\r\n        <div class=\"form-group\">\r\n          <button class=\"btnc  btn-block\" type=\"submit\">\r\n\r\n            <span>Login</span>\r\n          </button>\r\n         <br><a href=\"#\"> Mot de passe oublié ?</a>\r\n        </div>\r\n\r\n        <div class=\"form-group\">\r\n          <div v-if=\"message\" class=\"alert alert-danger\" role=\"alert\">\r\n            {{ message }}\r\n          </div>\r\n        </div>\r\n      </form>\r\n    </div>\r\n      <div class=\"text\">\r\n\r\n          <div class=\"bloci\">\r\n\r\n            <p>Vous avez déja un compte connectez-vous pour accéder à votre profil</p>\r\n            <div class=\"mybtn\">\r\n              <button  onclick=\"window.location.href = '/register';\" class=\"btn  btn-block\">\r\n            <span\r\n                v-show=\"loading\"\r\n                class=\"spinner-border spinner-border-sm\"\r\n            ></span>\r\n\r\n                <span>Inscription</span>\r\n\r\n              </button>\r\n\r\n            </div>\r\n\r\n          </div>\r\n\r\n\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport {  Field, ErrorMessage } from \"vee-validate\";\r\nimport * as yup from \"yup\";\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: \"LoginComponent\",\r\n  components: {\r\n    Field,\r\n    ErrorMessage,\r\n  },\r\n  data() {\r\n    const schema = yup.object().shape({\r\n      email: yup.string().required(\"Veiller entrer votre identifiant\"),\r\n      password: yup.string().required(\"Veiller entrer votre mot de passe\"),\r\n    });\r\n\r\n    return {\r\n      loading: false,\r\n      message: \"\",\r\n      schema,\r\n      email: '',\r\n      password: ''\r\n    };\r\n  },\r\n  methods: {\r\n    async onSubmit(){\r\n      try {\r\n        // Envoyer la requête d'authentification à l'API\r\n        const response = await axios.post('http://localhost:3000/signup', {\r\n          nom: this.nom,\r\n          prenom: this.prenom,\r\n          email: this.email,\r\n          mdp: this.mdp\r\n        });\r\n        // Traiter la réponse de l'API\r\n        \r\n        const data = await response.json();\r\n        console.log(data)\r\n        if (response.ok) {\r\n          \r\n          // Enregistrer le jeton d'authentification dans le stockage local ou dans un cookie\r\n          localStorage.setItem('authToken', data.token);\r\n\r\n          // Rediriger l'utilisateur vers la page suivante\r\n          // this.$router.push('/profil');\r\n        } else {\r\n          // Afficher un message d'erreur à l'utilisateur\r\n          alert(data.message);\r\n        }\r\n      } catch (error) {\r\n        console.error(error);\r\n      }\r\n    }\r\n  }\r\n\r\n\r\n\r\n};\r\n</script>\r\n\r\n<style>\r\n\r\n\r\n\r\n.containnerform{\r\n  margin:auto;\r\n  width: 50%;\r\n  height: 80%;\r\n}\r\n.bloc{\r\n  display: flex;\r\n  justify-content: space-between;\r\n  margin-top: 40px;\r\n  box-shadow: 10px 5px 5px gray ;\r\n  height: 500px;\r\n  width: 700px;\r\n}\r\n.form{\r\n  margin-right: 70px !important;\r\n  width: 500px;\r\n  margin-top: 20px;\r\n}\r\n.text{\r\n  width:80%;\r\n  height:100%;\r\n  background-color: forestgreen;\r\n}\r\n.bloci{\r\n  margin-top: 150px;\r\n  text-align:center ;\r\n  font-size: 18px;\r\n  padding: 20px;\r\n\r\n}\r\n\r\n.mybtn button{\r\n  width:30vh;\r\n  margin:auto;\r\n  background-color: white;\r\n\r\n}\r\n.form img{\r\n  width: 100px;\r\n  height: 100px;\r\n  margin-left:80px;\r\n}\r\n.text p{\r\n\r\n  color:white;\r\n}\r\n.titre{\r\nmargin-left:50px;\r\n\r\n}\r\n.btnc{\r\n  background-color: forestgreen;\r\n  height: 40px;\r\n  color: white;\r\n}\r\n@media screen and (max-width: 756px){\r\n  .containnerform{\r\n    height: 100vh;\r\n    width:40vh;\r\n  }\r\n  .bloc{\r\n    height:800px;\r\n    display:flex;\r\n    flex-direction:column ;\r\n  }\r\n  .form{\r\n    margin-right: 100px !important;\r\n    width: 280px;\r\n    margin-top: 20px;\r\n  }\r\n\r\n}\r\n\r\n</style>"],"mappings":"AAgEA,SAAUA,KAAK,EAAEC,YAAW,QAAS,cAAc;AACnD,OAAO,KAAKC,GAAE,MAAO,KAAK;AAC1B,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,UAAU,EAAE;IACVL,KAAK;IACLC;EACF,CAAC;EACDK,IAAIA,CAAA,EAAG;IACL,MAAMC,MAAK,GAAIL,GAAG,CAACM,MAAM,EAAE,CAACC,KAAK,CAAC;MAChCC,KAAK,EAAER,GAAG,CAACS,MAAM,EAAE,CAACC,QAAQ,CAAC,kCAAkC,CAAC;MAChEC,QAAQ,EAAEX,GAAG,CAACS,MAAM,EAAE,CAACC,QAAQ,CAAC,mCAAmC;IACrE,CAAC,CAAC;IAEF,OAAO;MACLE,OAAO,EAAE,KAAK;MACdC,OAAO,EAAE,EAAE;MACXR,MAAM;MACNG,KAAK,EAAE,EAAE;MACTG,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDG,OAAO,EAAE;IACP,MAAMC,QAAQA,CAAA,EAAE;MACd,IAAI;QACF;QACA,MAAMC,QAAO,GAAI,MAAMf,KAAK,CAACgB,IAAI,CAAC,8BAA8B,EAAE;UAChEC,GAAG,EAAE,IAAI,CAACA,GAAG;UACbC,MAAM,EAAE,IAAI,CAACA,MAAM;UACnBX,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBY,GAAG,EAAE,IAAI,CAACA;QACZ,CAAC,CAAC;QACF;;QAEA,MAAMhB,IAAG,GAAI,MAAMY,QAAQ,CAACK,IAAI,EAAE;QAClCC,OAAO,CAACC,GAAG,CAACnB,IAAI;QAChB,IAAIY,QAAQ,CAACQ,EAAE,EAAE;UAEf;UACAC,YAAY,CAACC,OAAO,CAAC,WAAW,EAAEtB,