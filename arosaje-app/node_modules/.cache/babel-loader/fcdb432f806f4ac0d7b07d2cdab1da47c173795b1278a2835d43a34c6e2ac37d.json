{"ast":null,"code":"import CommentCom from '@/components/CommentCom.vue';\nimport myContentDrawer from \"@/components/DrawerComponent.vue\";\nimport DrawerMobile from \"@/components/DrawerMobile.vue\";\nimport axios from 'axios';\nexport default {\n  components: {\n    CommentCom,\n    myContentDrawer,\n    DrawerMobile\n  },\n  name: \"MyDataApi\",\n  props: ['post'],\n  data() {\n    return {\n      posts: {},\n      show: false,\n      isMobile: false\n    };\n  },\n  created() {\n    axios.get('https://63f34dfbde3a0b242b3f57c3.mockapi.io/post').then(response => {\n      this.posts = response.data;\n      console.log(this.posts);\n    }).catch(error => {\n      console.log(error);\n    });\n    const mediaQuery = window.matchMedia(\"(max-width: 767px)\");\n    this.isMobile = mediaQuery.matches;\n    mediaQuery.addEventListener('change', event => {\n      this.isMobile = event.matches;\n      if (this.isMobile) {\n        // La taille de l'écran est inférieure à 768px\n        this.mobileScreenFunction();\n      } else {\n        // La taille de l'écran est supérieure ou égale à 768px\n        this.desktopScreenFunction();\n      }\n    });\n  },\n  methods: {\n    mobileScreenFunction() {\n      // Fonction à exécuter lorsque la taille de l'écran est inférieure à 768px\n      console.log(\"La taille de l'écran est inférieure à 768px\");\n    },\n    desktopScreenFunction() {\n      // Fonction à exécuter lorsque la taille de l'écran est supérieure ou égale à 768px\n      console.log(\"La taille de l'écran est supérieure ou égale à 768px\");\n    }\n  }\n};","map":{"version":3,"names":["CommentCom","myContentDrawer","DrawerMobile","axios","components","name","props","data","posts","show","isMobile","created","get","then","response","console","log","catch","error","mediaQuery","window","matchMedia","matches","addEventListener","event","mobileScreenFunction","desktopScreenFunction","methods"],"sources":["/Users/syalassanealy/Downloads/a-rosa-je_mspr_epsi/arosaje-app/src/views/ProfilView.vue"],"sourcesContent":["<template>\n  <v-container class=\"content_user\">\n    <v-container class=\"contenu\" v-if=\"isMobile\">\n      <DrawerMobile />\n    </v-container>\n    <v-container v-else class=\"contenu\" >\n      <myContentDrawer />\n    </v-container>\n\n    <v-container  :style=\"{background: $vuetify.theme.themes.dark}\" class=\"post\">\n      <v-container :key=\"index\" v-for=\"(item, index) in posts\" fluid >\n        <v-row>\n          <v-col cols=\"12\" sm=\"12\" md=\"12\" id=\"actu\">\n\n\n              <v-row  class=\"post\">\n                <v-list>\n\n                  <v-avatar class=\"avatar\" size=\"60\">\n                    <v-img :src=\"item.avatar\" alt=\"profile\" />\n\n                  </v-avatar>\n                  <div class=\"texte\">\n                    <h3>{{item.name}} </h3>\n                    <p class=\"grey--text\">{{item.description }}</p>\n                    <h5 class=\"grey--text\">Overview</h5>\n                  </div>\n\n                  <v-card\n                      class=\"mx-auto\"\n                      max-width=\"400\"\n                  >\n                    <v-img\n                        :src=\"item.image\"\n                        height=\"300px\"\n                        cover\n                    ></v-img>\n\n                  <CommentCom/>\n                  </v-card>\n                </v-list>\n              </v-row>\n\n\n          </v-col>\n\n        </v-row>\n      </v-container>\n    </v-container>\n  </v-container>\n\n\n</template>\n\n\n<script>\nimport CommentCom from '@/components/CommentCom.vue'\nimport myContentDrawer from \"@/components/DrawerComponent.vue\"\nimport DrawerMobile from \"@/components/DrawerMobile.vue\"\nimport axios from 'axios';\n\n\nexport default {\n\n  components: {CommentCom,myContentDrawer,DrawerMobile},\n\n  name:\"MyDataApi\",\n  props: ['post'],\n\n  data() {\n\n    return {\n      posts: {},\n      show: false,\n      isMobile: false,\n    };\n  },\n\n  created() {\n    axios.get('https://63f34dfbde3a0b242b3f57c3.mockapi.io/post')\n        .then(response => {\n          this.posts = response.data;\n          console.log(this.posts);\n        })\n        .catch(error => {\n          console.log(error);\n        });\n    const mediaQuery = window.matchMedia(\"(max-width: 767px)\");\n    this.isMobile = mediaQuery.matches;\n    mediaQuery.addEventListener('change', (event) => {\n      this.isMobile = event.matches;\n      if (this.isMobile) {\n        // La taille de l'écran est inférieure à 768px\n        this.mobileScreenFunction();\n      } else {\n        // La taille de l'écran est supérieure ou égale à 768px\n        this.desktopScreenFunction();\n      }\n    });\n  },\n\n  methods: {\n    mobileScreenFunction() {\n      // Fonction à exécuter lorsque la taille de l'écran est inférieure à 768px\n      console.log(\"La taille de l'écran est inférieure à 768px\");\n    },\n    desktopScreenFunction() {\n      // Fonction à exécuter lorsque la taille de l'écran est supérieure ou égale à 768px\n      console.log(\"La taille de l'écran est supérieure ou égale à 768px\");\n    },\n  },\n\n\n}\n</script>\n<style scoped>\n.contenu{\n  width:300px\n}\n\n#actu{\n width:600px;\n}\n.texte{\n  margin-left: 30px;\n}\n.content_user{\n  display: flex;\n  justify-content: space-around;\n}\n.avatar{\n  margin-left: 20px;\n  margin-top: 20px;\n}\n</style>"],"mappings":"AAwDA,OAAOA,UAAS,MAAO,6BAA4B;AACnD,OAAOC,eAAc,MAAO,kCAAiC;AAC7D,OAAOC,YAAW,MAAO,+BAA8B;AACvD,OAAOC,KAAI,MAAO,OAAO;AAGzB,eAAe;EAEbC,UAAU,EAAE;IAACJ,UAAU;IAACC,eAAe;IAACC;EAAY,CAAC;EAErDG,IAAI,EAAC,WAAW;EAChBC,KAAK,EAAE,CAAC,MAAM,CAAC;EAEfC,IAAIA,CAAA,EAAG;IAEL,OAAO;MACLC,KAAK,EAAE,CAAC,CAAC;MACTC,IAAI,EAAE,KAAK;MACXC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EAEDC,OAAOA,CAAA,EAAG;IACRR,KAAK,CAACS,GAAG,CAAC,kDAAkD,EACvDC,IAAI,CAACC,QAAO,IAAK;MAChB,IAAI,CAACN,KAAI,GAAIM,QAAQ,CAACP,IAAI;MAC1BQ,OAAO,CAACC,GAAG,CAAC,IAAI,CAACR,KAAK,CAAC;IACzB,CAAC,EACAS,KAAK,CAACC,KAAI